{
  "name": "boli-flow",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "electron build/electron/main.js",
    "build": "electron-builder -c electron-builder.config.ts",
    "flow": "flow",
    "lint": "eslint src --fix",
    "format": "prettier --write src/",
    "up": "npx taze major -I",
    "postinstall": "npx simple-git-hooks && electron-builder install-app-deps"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@app/cli": "workspace:*",
    "@vue/eslint-config-prettier": "catalog:",
    "@vue/eslint-config-typescript": "catalog:",
    "electron": "catalog:",
    "electron-builder": "^26.0.12",
    "eslint": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:",
    "vue-i18n": "catalog:"
  }
}
